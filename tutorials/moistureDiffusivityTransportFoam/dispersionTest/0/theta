FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      theta;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.025;

boundaryField
{
    frontAndBack
    {
        type            empty;
    }
    walls
    {
        type            zeroGradient;
    }
    inletU
    {
       type            fixedValue;
        value           uniform 0.6999999;
    }
    inletD
    {
      type            fixedValue;
      value           uniform 0.6999999;
    }
    outletD
    {
       type            zeroGradient;
    }
    outletU
    {
      type            zeroGradient;
    }
}



