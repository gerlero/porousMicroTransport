    frontAndBack
    {
        type            empty;
    }
    walls
    {
        type            zeroGradient;
    }
    inletU
    {
        type            fixedValue;
        value           uniform 0.7;
    }
    inletD
    {
        type            fixedValue;
        value           uniform 0.7;
    }
    outletD
    {
        type            zeroGradient;
    }
    outletU
    {
        type            zeroGradient;
    }
}
