// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
FoamFile
{
    version         2;
    format          ascii;
    class           dictionary;
    location        "system";
    object          controlDict;
}

application     porousMicroTransportFoam;

startFrom       latestTime;

stopAt          endTime;

endTime         50;

deltaT          1e-05;

writeControl    adjustableRunTime;

writeInterval   50;

purgeWrite      0;

writeFormat     ascii;

writePrecision  10;

writeCompression no;

timeFormat      general;

timePrecision   6;

adjustTimeStep  yes;

runTimeModifiable no;

maxDeltaT       .25;


