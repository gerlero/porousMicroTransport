FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}

application     moistureDiffusivityFoam;

startFrom       firstTime;

stopAt          nextWrite;

deltaT          1e-5;

writeControl    adjustableRunTime;

writeInterval   100;

purgeWrite      0;

writeFormat     ascii;

writePrecision  10;

writeCompression no;

timeFormat      general;

timePrecision   6;

adjustTimeStep  yes;

runTimeModifiable no;
