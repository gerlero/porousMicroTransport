FoamFile
{
    version         2;
    format          ascii;
    class           dictionary;
    location        "system";
    object          controlDict;
}

application     porousMicroTransportFoam;

startFrom       latestTime;

stopAt          endTime;

endTime         30;

deltaT          1e-05;

writeControl    adjustableRunTime;

writeInterval   30;

purgeWrite      0;

writeFormat     ascii;

writePrecision  10;

writeCompression no;

timeFormat      general;

timePrecision   6;

adjustTimeStep  yes;

runTimeModifiable no;

maxDeltaC       1e-2;
