boundaryField
{
    left
    {
        type            zeroGradient;
    }
    right 
    {
        type            zeroGradient;
    }
    top
    {
        type            empty;
    }
    bottom
    {
       type             empty;
    }
    front
    {
        type            empty;
    }
    back
    {
        type            empty;
    }
}
